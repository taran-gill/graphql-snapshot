{
  "name": "audit-graph",
  "version": "0.0.1",
  "description": "Test and snapshot tool for verifying your GraphQL Api works",
  "main": "src/index.js",
  "scripts": {
    "build:prod": "webpack --config config/webpack/webpack.prod.js",
    "build:dev": "webpack --config config/webpack/webpack.dev.js",
    "start": "yarn run build:dev && node ./test/server/index.js",
    "test": "yarn run build:dev && ./node_modules/.bin/jest --config config/jest/jest.config.js"
  },
  "author": "Taran Gill",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.0",
    "apollo-server": "^2.8.1",
    "apollo-server-testing": "^2.8.1",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "jest-specific-snapshot": "^2.0.0",
    "raw-loader": "^3.1.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "json-to-graphql-query": "^1.9.0"
  }
}
