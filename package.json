    "apollo-server-testing": "^2.8.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "jest-specific-snapshot": "^2.0.0",
    "jest-transform-graphql": "^2.1.0",
    "raw-loader": "^3.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
